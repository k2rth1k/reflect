{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron: Main",
      "runtimeExecutable": "electron",
      "program": "${workspaceFolder}/.webpack/main",
      "args": [],
      "cwd": "${workspaceFolder}",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "preLaunchTask": "npm: start",
      "env": {
        "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
      },
      "outputCapture": "std"
    }
  ]
}